;;;(courseIdentifier ?course ?id)
(isa courseIdentifier Predicate)
(arity courseIdentifier 2)
(arg1Isa courseIdentifier NUCourse-CS)
(arg2Isa courseIdentifier CourseID)
(comment courseIdentifier "(courseIdentifier ?course ?id) establishes relationship between quarter-specific ?course and unique ?id")

;;; (coursePrereq ?course ?prereq)
(isa coursePrereq Predicate)
(arity coursePrereq 2)
(arg1Isa coursePrereq CourseID)
(arg2Isa coursePrereq CourseID)
(comment coursePrereq "(coursePrereq ?course ?prereq) indicates that ?prereq is the prerequisite of a given Course-Collegiate ?course.")

;;; (courseTaken ?StudentID ?CourseID)
(isa courseTaken Predicate)
(isa courseTaken CycLSentence-Assertible)
(arity courseTaken 2)
(arg1Isa courseTaken StudentID)
(arg2Isa courseTaken CourseID)
(comment courseTaken "Predicate to represent that a student has taken a specific course")

(<== (courseTaken ?studentID EECS211andEECS214)
    (courseTaken ?studentID EECS211)
    (courseTaken ?studentID EECS214)
)

(<== (courseTaken ?studentID EECS212andEECS214)
    (courseTaken ?studentID EECS212)
    (courseTaken ?studentID EECS214)
)

(<== (courseTaken ?studentID EECS213andEECS214)
    (courseTaken ?studentID EECS213)
    (courseTaken ?studentID EECS214)
)

(<== (courseTaken ?studentID EECS213andEECS214)
    (courseTaken ?studentID EECS213)
    (courseTaken ?studentID EECS214)
)

(<== (courseTaken ?studentID EECS212andEECS213andEECS214)
    (courseTaken ?studentID EECS212)
    (courseTaken ?studentID EECS213)
